{
  "name": "kbbi-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "pre-commit": "lint-staged",
    "clone-kbbi": "rm -rf public/kbbi && git clone https://github.com/hanjoyo/kbbi public/kbbi",
    "build:word": "node scripts/generate-word-mapping.js",
    "build:pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg ",
    "build:app": "tsc -b && vite build",
    "build:sitemap": "node scripts/generate-sitemap.js",
    "build": "run-s build:*",
    "build-prod": "run-s clone-kbbi build:*"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^1.0.2"
  }
}